{
    "name": "Libstrophe",
    "description": "XMPP library for C",
    "url": "https://github.com/strophe/libstrophe/archive/0.9.2.tar.gz",
    "mirror": null,
    "homepage": "http://strophe.im/libstrophe/",
    "depends": [
        {
            "depend": "autoconf",
            "build-depend": true
        },
        {
            "depend": "automake",
            "build-depend": true
        },
        {
            "depend": "libtool",
            "build-depend": true
        },
        {
            "depend": "pkg-config",
            "build-depend": true
        },
        {
            "depend": "check",
            "build-depend": false
        },
        {
            "depend": "openssl",
            "build-depend": false
        }
    ],
    "resource": [],
    "conflicts": [],
    "patches": [],
    "install": [
        "system \"./bootstrap.sh\"",
        "system \"./configure\", \"--disable-dependency-tracking\",",
        "\"--disable-silent-rules\",",
        "\"--prefix=#{prefix}\"",
        "system \"make\", \"install\""
    ],
    "version": "0.9.2"
}