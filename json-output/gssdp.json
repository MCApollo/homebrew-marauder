    "url": "https://download.gnome.org/sources/gssdp/1.0/gssdp-1.0.3.tar.xz",
    "mirror": null,
    "homepage": "https://wiki.gnome.org/GUPnP/",
    "depends": [
        {
            "depend": "intltool",
            "build-depend": true
        },
        {
