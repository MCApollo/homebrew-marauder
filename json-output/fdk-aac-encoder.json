{
    "name": "FdkAacEncoder",
    "description": "Command-line encoder frontend for libfdk-aac",
    "url": "https://github.com/nu774/fdkaac/archive/1.0.0.tar.gz",
    "mirror": null,
    "homepage": "https://github.com/nu774/fdkaac",
    "depends": [
        {
            "depend": "autoconf",
            "build-depend": true
        },
        {
            "depend": "automake",
            "build-depend": true
        },
        {
            "depend": "libtool",
            "build-depend": true
        },
        {
            "depend": "pkg-config",
            "build-depend": true
        },
        {
            "depend": "fdk-aac",
            "build-depend": false
        }
    ],
    "resource": [],
    "conflicts": [],
    "patches": [],
    "install": [
        "system \"autoreconf\", \"-i\"",
        "system \"./configure\", \"--disable-debug\",",
        "\"--disable-dependency-tracking\",",
        "\"--disable-silent-rules\",",
        "\"--prefix=#{prefix}\",",
        "\"--mandir=#{man}\"",
        "system \"make\", \"install\""
    ],
    "version": "1.0.0"
}