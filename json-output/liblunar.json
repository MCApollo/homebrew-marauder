    "url": "https://storage.googleapis.com/google-code-archive-downloads/v2/code.google.com/liblunar/liblunar-2.2.5.tar.gz",
    "mirror": null,
    "homepage": "https://code.google.com/archive/p/liblunar/",
    "depends": [
        {
            "depend": "intltool",
            "build-depend": true
        },
        {
