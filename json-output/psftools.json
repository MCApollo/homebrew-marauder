{
    "name": "Psftools",
    "description": "Tools for fixed-width bitmap fonts",
    "url": "https://www.seasip.info/Unix/PSF/psftools-1.1.10.tar.gz",
    "mirror": null,
    "homepage": "https://www.seasip.info/Unix/PSF/",
    "depends": [
        {
            "depend": "autoconf",
            "build-depend": true
        }
    ],
    "resource": [
        {
            "name": "pc8x8font",
            "url": "https://www.zone38.net/font/pc8x8.zip"
        }
    ],
    "conflicts": [],
    "patches": [],
    "install": [
        "system \"autoconf\"",
        "system \"./configure\", \"--prefix=#{prefix}\", \"--mandir=#{man}\"",
        "system \"make\", \"install\""
    ],
    "version": "1.1.10"
}