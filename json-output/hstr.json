{
    "name": "Hstr",
    "description": "Bash and zsh history suggest box",
    "url": "https://github.com/dvorka/hstr/archive/2.0.tar.gz",
    "mirror": null,
    "homepage": "https://github.com/dvorka/hstr",
    "depends": [
        {
            "depend": "autoconf",
            "build-depend": true
        },
        {
            "depend": "automake",
            "build-depend": true
        },
        {
            "depend": "libtool",
            "build-depend": true
        },
        {
            "depend": "pkg-config",
            "build-depend": true
        },
        {
            "depend": "readline",
            "build-depend": false
        }
    ],
    "resource": [],
    "conflicts": [],
    "patches": [],
    "install": [
        "system \"autoreconf\", \"-fvi\"",
        "system \"./configure\", \"--disable-dependency-tracking\",",
        "\"--prefix=#{prefix}\"",
        "system \"make\", \"install\""
    ],
    "version": "2.0"
}