{
    "name": "PerconaToolkit",
    "description": "Percona Toolkit for MySQL",
    "url": "https://www.percona.com/downloads/percona-toolkit/3.0.13/source/tarball/percona-toolkit-3.0.13.tar.gz",
    "mirror": null,
    "homepage": "https://www.percona.com/software/percona-toolkit/",
    "depends": [
        {
            "depend": "mysql-client",
            "build-depend": false
        },
        {
            "depend": "openssl",
            "build-depend": false
        }
    ],
    "resource": [
        {
            "name": "DBI",
            "url": "https://cpan.metacpan.org/authors/id/T/TI/TIMB/DBI-1.642.tar.gz"
        },
        {
            "name": "DBD::mysql",
            "url": "https://cpan.metacpan.org/authors/id/C/CA/CAPTTOFU/DBD-mysql-4.046.tar.gz"
        },
        {
            "name": "JSON",
            "url": "https://cpan.metacpan.org/authors/id/I/IS/ISHIGAKI/JSON-4.00.tar.gz"
        }
    ],
    "conflicts": [],
    "patches": [],
    "install": [
        "ENV.prepend_create_path \"PERL5LIB\", libexec/\"lib/perl5\"",
        "resources.each do |r|",
        "r.stage do",
        "system \"perl\", \"Makefile.PL\", \"INSTALL_BASE=#{libexec}\"",
        "system \"make\", \"install\""
    ],
    "version": "3.0.13"
}