{
    "name": "I386ElfBinutils",
    "description": "FSF Binutils for i386-elf cross development",
    "url": "https://ftp.gnu.org/gnu/binutils/binutils-2.32.tar.gz",
    "mirror": "https://ftpmirror.gnu.org/binutils/binutils-2.32.tar.gz",
    "homepage": "https://www.gnu.org/software/binutils/",
    "depends": [],
    "resource": [],
    "conflicts": [],
    "patches": [],
    "install": [
        "system \"./configure\", \"--target=i386-elf\",",
        "\"--disable-multilib\",",
        "\"--disable-nls\",",
        "\"--disable-werror\",",
        "\"--prefix=#{prefix}\"",
        "system \"make\"",
        "system \"make\", \"install\""
    ],
    "version": "2.32"
}