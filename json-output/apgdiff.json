{
    "name": "Apgdiff",
    "description": "Another PostgreSQL diff tool",
    "url": "https://www.apgdiff.com/download/apgdiff-2.4-bin.zip",
    "mirror": null,
    "homepage": "https://www.apgdiff.com/",
    "depends": [
        {
            "depend": "ant",
            "build-depend": true
        }
    ],
    "resource": [],
    "conflicts": [],
    "patches": [],
    "install": [
        "jar = \"apgdiff-#{version}.jar\"",
        "if build.head?",
        "system \"ant\"",
        "cd \"dist\" do",
        "jar = Dir[\"apgdiff-*.jar\"].first",
        "mv jar, \"..\"",
        "end"
    ],
    "version": "2.4"
}