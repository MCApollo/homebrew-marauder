    "conflicts": [],
    "patches": [],
    "install": [
        "ENV.prepend_create_path \"PERL5LIB\", libexec+\"lib/perl5\"",
        "build_pl = [\"JSON::WebToken\", \"Module::Build::Tiny\", \"Readonly\"]",
        "resources.each do |r|",
        "r.stage do",
        "next if build_pl.include? r.name",
        "system \"perl\", \"Makefile.PL\", \"INSTALL_BASE=#{libexec}\"",
        "system \"make\"",
        "system \"make\", \"install\""
    ],
    "version": "1.882"
